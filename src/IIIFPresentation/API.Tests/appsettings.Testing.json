{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "RunMigrations": true,
  "ResourceRoot": "https://localhost:7230",
  "PageSize": 20,
  "MaxPageSize": 100,
  "AWS": {
    "S3": {
      "StorageBucket": "presentation-storage"
    }
  }
}
