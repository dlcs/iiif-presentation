{
  "RunMigrations": true,
  "ResourceRoot": "https://localhost:7230",
  "PageSize": 20,
  "MaxPageSize": 100,
  "AWS": {
    "S3": {
      "StorageBucket": "presentation-storage"
    }
  },
  "DLCS": {
    "ApiUri": "https://localhost:6000",
    "OrchestratorUri": "https://localhost:6010",
    "MaxBatchSize": 2
  },
  "PathSettings": {
    "PresentationApiUrl": "https://localhost:7230/",
    "CustomerPresentationApiUrl": {
      "601": "https://example.com",
      "602": "https://no-customer.com"
    },
    "PathRules": {
      "Defaults": {
        "ResourcePublic": "/{customerId}/{hierarchyPath}",
        "ManifestPrivate": "/{customerId}/manifests/{resourceId}",
        "CollectionPrivate": "/{customerId}/collections/{resourceId}",
        "Canvas": "/{customerId}/canvases/{resourceId}"
      },
      "Overrides": {
        "example.com": {
          "ResourcePublic": "/example/{customerId}/{hierarchyPath}",
          "ManifestPrivate": "/example/{customerId}/manifests/{resourceId}",
          "CollectionPrivate": "/foo/{customerId}/collections/{resourceId}",
          "Canvas": "/example/{customerId}/canvases/{resourceId}"
        },
        "no-customer.com": {
          "ResourcePublic": "/{hierarchyPath}",
          "ManifestPrivate": "/manifests/{resourceId}",
          "CollectionPrivate": "/collections/{resourceId}",
          "Canvas": "/canvases/{resourceId}"
        },
        "no-customer-additional-path-element.com": {
          "ResourcePublic": "/test/{hierarchyPath}",
          "ManifestPrivate": "/test/manifests/{resourceId}",
          "CollectionPrivate": "/test/collections/{resourceId}",
          "Canvas": "/test/canvases/{resourceId}"
        },
        "no-customer-multiple-path-element.com": {
          "ResourcePublic": "/has/multiple/additional/elements/{hierarchyPath}",
          "ManifestPrivate": "/has/multiple/additional/elements/manifests/{resourceId}",
          "CollectionPrivate": "/has/multiple/additional/elements/collections/{resourceId}",
          "Canvas": "/has/multiple/additional/elements/canvases/{resourceId}"
        },
        "fully-qualified.com": {
          "ResourcePublic": "https://fully_qualified.co.uk/{customerId}/{hierarchyPath}",
          "ManifestPrivate": "https://fully_qualified.co.uk/{customerId}/manifests/{resourceId}",
          "CollectionPrivate": "https://fully_qualified.co.uk/{customerId}/collections/{resourceId}",
          "Canvas": "https://fully_qualified.co.uk/{customerId}/canvases/{resourceId}"
        },
        "fully-qualified-no-customer.com": {
          "ResourcePublic": "https://fully_qualified_no_customer.co.uk/{hierarchyPath}",
          "ManifestPrivate": "https://fully_qualified_no_customer.co.uk/manifests/{resourceId}",
          "CollectionPrivate": "https://fully_qualified_no_customer.co.uk/collections/{resourceId}",
          "Canvas": "https://fully_qualified_no_customer.co.uk/canvases/{resourceId}"
        }
      }
    }
  }
}
